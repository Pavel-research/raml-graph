#%RAML 1.0
title: Hello
uses:
 s: shapes.raml
mediaType: application/json
types:
  Organization:
     properties:
       id: string
       name: string
  Repository:
    properties:
      id: integer
      name: string
      lastName: string
      org: Organization
  RepositoryData:
     (s.shapeOf): Repository
     properties:
       id: integer
       name: string
/repositories:
  /orgs/{orgId}/{id}:
     uriParameters:
       orgId:
        (s.reference): Organization.id
       id:
        (s.reference): Repository.id
     get:
       responses:
         200:
           body: Repository